{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "radix": 0,
    "strict": 0,
    "new-cap": 0,
    "func-names": 0,
    "no-console": 2,
    "global-require": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "prefer-rest-params": 0,
    "no-unused-expressions": 0,
    "space-before-function-paren": 0,
    "no-param-reassign": [2, { "props": false }],
  }
}
